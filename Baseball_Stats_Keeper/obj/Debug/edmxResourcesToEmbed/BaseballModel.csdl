<?xml version="1.0" encoding="utf-8"?>
<Schema Namespace="practiceDatabaseModel" Alias="Self" annotation:UseStrongSpatialTypes="false" xmlns:annotation="http://schemas.microsoft.com/ado/2009/02/edm/annotation" xmlns:customannotation="http://schemas.microsoft.com/ado/2013/11/edm/customannotation" xmlns="http://schemas.microsoft.com/ado/2009/11/edm">
  <EntityType Name="Player">
    <Key>
      <PropertyRef Name="PlayerID" />
    </Key>
    <Property Name="PlayerID" Type="Int32" Nullable="false" />
    <Property Name="FirstName" Type="String" MaxLength="15" FixedLength="true" Unicode="true" Nullable="false" />
    <Property Name="LastName" Type="String" MaxLength="25" FixedLength="true" Unicode="true" Nullable="false" />
    <Property Name="Grade" Type="Int32" Nullable="false" />
    <Property Name="Team" Type="String" MaxLength="20" FixedLength="true" Unicode="true" Nullable="false" />
    <NavigationProperty Name="Stats" Relationship="Self.FK_Stats_Players" FromRole="Players" ToRole="Stats" />
  </EntityType>
  <EntityType Name="Stat">
    <Key>
      <PropertyRef Name="StatID" />
    </Key>
    <Property Name="StatID" Type="Int32" Nullable="false" />
    <Property Name="PlayerID" Type="Int32" Nullable="false" />
    <Property Name="AtBats" Type="Int32" />
    <Property Name="Hits" Type="Int32" />
    <Property Name="StrikeOuts" Type="Int32" />
    <Property Name="Walks" Type="Int32" />
    <Property Name="Singles" Type="Int32" />
    <Property Name="Doubles" Type="Int32" />
    <Property Name="Triples" Type="Int32" />
    <Property Name="Homeruns" Type="Int32" />
    <Property Name="RBI" Type="Int32" />
    <Property Name="Runs" Type="Int32" />
    <Property Name="Average" Type="Int32" />
    <NavigationProperty Name="Player" Relationship="Self.FK_Stats_Players" FromRole="Stats" ToRole="Players" />
  </EntityType>
  <Association Name="FK_Stats_Players">
    <End Role="Players" Type="Self.Player" Multiplicity="1" />
    <End Role="Stats" Type="Self.Stat" Multiplicity="*" />
    <ReferentialConstraint>
      <Principal Role="Players">
        <PropertyRef Name="PlayerID" />
      </Principal>
      <Dependent Role="Stats">
        <PropertyRef Name="PlayerID" />
      </Dependent>
    </ReferentialConstraint>
  </Association>
  <EntityContainer Name="practiceDatabaseEntities" annotation:LazyLoadingEnabled="true">
    <EntitySet Name="Players" EntityType="Self.Player" />
    <EntitySet Name="Stats" EntityType="Self.Stat" />
    <AssociationSet Name="FK_Stats_Players" Association="Self.FK_Stats_Players">
      <End Role="Players" EntitySet="Players" />
      <End Role="Stats" EntitySet="Stats" />
    </AssociationSet>
  </EntityContainer>
</Schema>